<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="NegativeTestsSuite" verbose="1">
    <listeners>
        <listener class-name="com.hotline.tests.listeners.TestListener"></listener>
    </listeners>
    <test name="Hotline Negative Tests">
        <groups>
            <run>
                <include name="negative"/>
            </run>
        </groups>
        <classes>
            <class name="com.hotline.tests.scenarios.NegativeTests"/>
        </classes>
    </test>
</suite>